[Desktop Entry]
Name=arping
Comment=Sends arp and/or ip pings to a given host
Encoding=UTF-8
Exec=shell-rocket -c "nix-shell -p iputils --command 'arping --help; return'"
Icon=athena-arping
StartupNotify=false
Terminal=false
Type=Application
Categories=01-02-identify-live-hosts;
X-Nix-Package=arping
