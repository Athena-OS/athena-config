[Desktop Entry]
Name=recordmydesktop
Comment=Record desktop sessions to an Ogg-Theora-Vorbis file.
Encoding=UTF-8
Exec=shell-rocket -c "nix-shell -p recordmydesktop --command 'recordmydesktop -h; return'"
Icon=athena-recordmydesktop
StartupNotify=false
Terminal=false
Type=Application
Categories=12-reporting;
X-Nix-Package=recordmydesktop
