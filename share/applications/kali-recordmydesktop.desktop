[Desktop Entry]
Name=recordmydesktop
Comment=Record desktop sessions to an Ogg-Theora-Vorbis file.
Encoding=UTF-8
Exec=shell-rocket "nix-shell -p recordmydesktop --command 'recordmydesktop -h; return'"
Icon=kali-recordmydesktop
StartupNotify=false
Terminal=false
Type=Application
Categories=12-reporting;
X-Nix-Package=recordmydesktop
