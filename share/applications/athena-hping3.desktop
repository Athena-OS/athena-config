[Desktop Entry]
Name=hping3
Comment=Send (almost) arbitrary TCP/IP packets to network hosts
Encoding=UTF-8
Exec=shell-rocket -c "nix-shell -p hping --command 'hping3 -h; return'"
Icon=athena-hping3
StartupNotify=false
Terminal=false
Type=Application
Categories=01-02-identify-live-hosts;
X-Nix-Package=hping3
