[Desktop Entry]
Name=recoverjpeg
Comment=Recover jpeg pictures from a filesystem image
Encoding=UTF-8
Exec=shell-rocket "nix-shell -p recoverjpeg --command 'recoverjpeg -h; return'"
Icon=athenaos-blue-net
StartupNotify=false
Terminal=false
Type=Application
Categories=11-05-forensic-carving-tools;005-recover;
X-Nix-Package=recoverjpeg
