[Desktop Entry]
Name=nmap
Comment=Network exploration tool and security / port scanner
Encoding=UTF-8
Exec=shell-rocket "nix-shell -p nmap --command 'nmap;$SHELL --command 'fi"
Icon=kali-nmap
